<div class="gallery">
    <div class="photoset">
        <% if (item.photos.length){ %>
        <% item.photos.forEach(function(photo){ %>
        <a href="<%- item.permalink %>"><%- image_tag(item.permalink + photo, {alt: 'photo'}) %></a>
        <% }); %>
        <% } else { %>
        <img src="<%- item.photos %>">
        <% } %>
    </div>
    <div class="control">
        <div class="prev"></div>
        <div class="next"></div>
    </div>
</div>